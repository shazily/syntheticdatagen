        "ollamaApi": {
          "id": "ollama-credentials",
          "name": "Ollama API"
        }
      }
    },
    {
      "parameters": {
        "responseBody": "={\n  \"status\": \"success\",\n  \"input\": $('Webhook').item.json,\n  \"embeddingOutput\": $json,\n  \"embeddingLength\": $json.embedding ? $json.embedding.length : 'no embedding found'\n}"
      },
      "id": "respond",
      "name": "Respond",
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1,
      "position": [
        680,
        300
      ]
    }
